= extend('/slm/layouts/default')

- title = 'Sticky'

/ Partials
= partial('/slm/partials/head.mixin.slm');
= partial('/slm/partials/content-header.mixin.slm');
= partial('/slm/partials/section.mixin.slm');


/ Content blocks
= content('head');
  = mixin('head');

= content('main')
  = mixin('content-header', title);
  div
    = mixin('section', title, 'utilities/sticky/sticky');

= content('script')
  javascript:
    gunyc.sticky();
    gunyc.sectionHighlighter();


